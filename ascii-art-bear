#include<iostream>

int main(){

	std::cout << "     ()____()" << std::endl;
	std::cout << "     / ___  \\" << std::endl;
	std::cout << "     \\ o o  /" << std::endl;
	std::cout << "     (_()_ )\\" << std::endl; 
	std::cout << "    /  ___   \\" << std::endl;
	std::cout << "   /  /   \\   \\" << std::endl;
	std::cout << "  ()  |   |   ()" << std::endl;
	std::cout << "    / |   |  \\" << std::endl;
	std::cout << "    ) \\___)  /" << std::endl;
	std::cout << "   ((_/  ((_/";

	return 0;
}
